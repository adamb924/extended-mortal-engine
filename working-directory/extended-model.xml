<?xml version="1.0" encoding="UTF-8"?>
<morphology
    xmlns="https://www.adambaker.org/mortal-engine"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://www.adambaker.org/mortal-engine morphology.xsd">
    <writing-systems src="writing-systems.xml"/>
    <model label="Extended Model">
        <stem-list label="Stem">
            <filename>stems.xml</filename>
        </stem-list>
        <morpheme label="Even Only">
            <optional/>
            <allomorph>
                <form lang="wk-LA">Steven</form>
                <preceding-letters-even/>
            </allomorph>
        </morpheme>
        <reduplicator label="Redup" optional="true"></reduplicator>
    </model>
</morphology>
